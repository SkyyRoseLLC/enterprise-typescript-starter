{
  "name": "enterprise-typescript-starter",
  "version": "1.0.0",
  "description": "Enterprise-ready TypeScript full-stack starter with advanced AI agent architecture",
  "main": "dist/backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --exec tsx src/backend/server.ts",
    "dev:frontend": "cd src/frontend && vite",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc --project tsconfig.backend.json",
    "build:frontend": "cd src/frontend && vite build",
    "start": "node dist/backend/server.js",
    "start:prod": "NODE_ENV=production npm start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:frontend": "cd src/frontend && tsc --noEmit",
    "clean": "rimraf dist coverage .eslintcache",
    "docker:build": "docker build -t enterprise-typescript-starter .",
    "docker:run": "docker run -p 3000:3000 enterprise-typescript-starter",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "agent:api": "ts-node src/agents/api/core/src/agents/backend/api-agent.ts",
    "agent:orchestration": "ts-node src/agents/api/core/src/agents/backend/orchestration-agent.ts",
    "agent:worker": "ts-node src/agents/api/core/src/agents/backend/worker-agent.ts",
    "agent:notification": "ts-node src/agents/api/core/src/agents/backend/notification-agent.ts",
    "agent:auth": "ts-node src/agents/api/core/src/agents/backend/authentication-agent.ts",
    "agent:monitoring": "ts-node src/agents/api/core/src/agents/backend/monitoring-agent.ts"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "express",
    "react",
    "full-stack",
    "enterprise",
    "docker",
    "ci-cd",
    "ai-agents",
    "microservices"
  ],
  "author": "SkyyRoseLLC",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "axios": "^1.6.2",
    "node-fetch": "^3.3.2",
    "bullmq": "^4.15.4",
    "ioredis": "^5.3.2",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.3",
    "firebase-admin": "^11.11.1",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.7.0",
    "passport-jwt": "^4.0.1",
    "winston": "^3.11.0",
    "pino": "^8.16.2",
    "prometheus-api-metrics": "^3.2.2",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/instrumentation-express": "^0.34.1",
    "@opentelemetry/instrumentation-http": "^0.45.1",
    "config": "^3.3.9",
    "inversify": "^6.0.2",
    "reflect-metadata": "^0.1.13",
    "eventemitter3": "^5.0.1",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/joi": "^17.2.3",
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.15",
    "@types/passport-jwt": "^3.0.13",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "playwright": "^1.40.1",
    "@playwright/test": "^1.40.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/parser": "^6.13.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}