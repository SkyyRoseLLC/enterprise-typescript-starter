{
  "name": "enterprise-typescript-starter",
  "version": "1.0.0",

  "description": "Enterprise-ready TypeScript full-stack starter with production best practices, CI/CD, Docker, and modular architecture",
  "main": "dist/backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --exec tsx src/backend/server.ts",
    "dev:frontend": "cd src/frontend && vite",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc --project tsconfig.backend.json",
    "build:frontend": "cd src/frontend && vite build",
    "start": "node dist/backend/server.js",
    "start:prod": "NODE_ENV=production npm start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:frontend": "cd src/frontend && tsc --noEmit",
    "clean": "rimraf dist coverage .eslintcache",
    "docker:build": "docker build -t enterprise-typescript-starter .",
    "docker:run": "docker run -p 3000:3000 enterprise-typescript-starter",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "express",
    "react",
    "full-stack",
    "enterprise",
    "docker",
    "ci-cd"
  ],
  "author": "SkyyRoseLLC",

  "description": "Enterprise-ready TypeScript full-stack starter with advanced AI agent architecture",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run build:client",
    "build:client": "cd client && npm run build",
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/index.ts",
    "dev:client": "cd client && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts client/src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.ts client/src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.ts client/src/**/*.{ts,tsx}",
    "docker:build": "docker build -t enterprise-typescript-starter .",
    "docker:run": "docker run -p 3000:3000 enterprise-typescript-starter",
    "install:all": "npm install && cd client && npm install",
    "agent:api": "ts-node src/agents/api/examples/run-example.ts",
    "agent:orchestration": "ts-node src/agents/orchestration/examples/run-example.ts",
    "agent:worker": "ts-node src/agents/background/examples/run-example.ts",
    "agent:notification": "ts-node src/agents/notification/examples/run-example.ts",
    "agent:auth": "ts-node src/agents/authentication/examples/run-example.ts",
    "agent:monitoring": "ts-node src/agents/monitoring/examples/run-example.ts"
  },
  "keywords": [
    "typescript",
    "enterprise",
    "agents",
    "ai",
    "production",
    "scalable",
    "architecture",
    "microservices",
    "full-stack"
  ],
  "author": "Enterprise Development Team",

  "license": "MIT",
